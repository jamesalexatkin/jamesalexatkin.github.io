{
    "title": "Creative",
    "name": "startbootstrap-creative",
    "version": "5.1.3",
    "scripts": {
        "start": "gulp watch",
        "build": "npm run build:clean && npm run build:vendor && npm run build:css && npm run build:js",
        "build:vendor": "node scripts/copy-vendor.js",
        "build:css": "sass --load-path=node_modules scss/creative.scss:css/creative.css --style=expanded && node scripts/minify-css.js",
        "build:js": "uglifyjs js/creative.js -o js/creative.min.js -c -m",
        "build:clean": "rmdir /s /q vendor 2>nul || true"
    },
    "description": "Creative is a stylish one page HTML theme for Bootstrap.",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://startbootstrap.com/template-overviews/creative",
    "bugs": {
        "url": "https://github.com/BlackrockDigital/startbootstrap-creative/issues",
        "email": "feedback@startbootstrap.com"
    },
    "license": "MIT",
    "author": "Start Bootstrap",
    "contributors": [
        "David Miller (http://davidmiller.io/)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/BlackrockDigital/startbootstrap-creative.git"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.4.0",
        "bootstrap": "^5.3.8",
        "jquery": "3.5.0",
        "jquery.easing": "^1.4.1",
        "magnific-popup": "^1.1.0",
        "sass": "^1.55.0"
    },
    "devDependencies": {
        "browser-sync": "^3.0.2",
        "del": "^3.0.0",
        "gulp": "5.0.0",
        "gulp-autoprefixer": "8.0.0",
        "gulp-clean-css": "4.0.0",
        "gulp-header": "2.0.7",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "1.4.0",
        "gulp-sass": "^5.1.0",
        "gulp-uglify": "3.0.1",
        "merge-stream": "^1.0.1"
    }
}
